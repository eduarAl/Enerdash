<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.enerdash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_enerdash_icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_enerdash_icono_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OnboardingActivity" android:theme="@style/AppOnboardingTheme"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".LogInActivity"/>
        <activity android:name=".CatalogoActivity"/>
    </application>
</manifest>